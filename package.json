{
  "name": "book-cricket",
  "version": "1.0.0",
  "description": "A simple game of Book Cricket",
  "main": "index.js",
  "scripts": {
    "build:dev": "SET NODE_ENV=local && webpack --config webpack.dev.js",
    "start:dev": "SET NODE_ENV=development && webpack-dev-server --config ./webpack.dev.js --hot --inline && webpack --config webpack.dev.js --watch",
    "build:prod": "SET NODE_ENV=production && webpack --config webpack.prod.js",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "build:webpack": "webpack --config webpack.dev.js",
    "netlify:build": "npm run build:webpack",
    "netlify:test+build": "jest --coverage && webpack --config webpack.dev.js"
  },
  "repository": {
    "url": "https://github.com/ClydeDz/book-cricket"
  },
  "author": "Clyde D'Souza",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/jest": "^24.0.18",
    "@types/jquery": "^3.3.31",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "ignore-styles": "^5.0.1",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "sass": "^1.17.2",
    "style-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
